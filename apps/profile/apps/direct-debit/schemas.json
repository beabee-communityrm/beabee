{
	"cancelSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["satisfied", "reason"],
			"properties": {
				"satisfied": {
					"type": "integer",
					"minimum": 1,
					"maximum": 5
				},
				"reason": {
					"type": "string",
					"minLength": 1
				},
				"other": {
					"type": "string"
				}
			}
		}
	},
	"updateSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["amount", "prorate"],
			"properties": {
				"amount": {
					"type": "integer",
					"minimum": 1
				},
				"prorate": {
					"type": "boolean"
				},
				"period": {
					"type": "string",
					"enum": ["monthly", "annually"]
				},
				"useMandate": {
					"type": "boolean"
				}
			}
		}
	},
	"completeFlowSchema": {
		"query": {
			"type": "object",
			"required": ["redirect_flow_id"],
			"properties": {
				"redirect_flow_id": {
					"type": "string"
				}
			}
		}
	}
}
