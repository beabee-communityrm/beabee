.row
	.col-md-12
		.table-responsive
			table.table.table-striped.table-condensed.table-hover
				tbody
					each member, m in members
						tr
							td(style='white-space: nowrap')= member.firstname + ' ' + member.lastname
							td(width="100%")= member.description
							td: a(href='/members/' + member.uuid).btn.btn-primary.btn-sm View
