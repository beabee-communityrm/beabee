{
	"createGiftSchema": {
		"body": {
			"type": "object",
			"required": ["firstname", "lastname", "email", "startDate", "fromName"],
			"properties": {
				"firstname": {
					"type": "string",
					"minLength": 1
				},
				"lastname": {
					"type": "string",
					"minLength": 1
				},
				"email": {
					"type": "string",
					"minLength": 1
				},
				"startDate": {
					"type": "string",
					"format": "date"
				},
				"message": {
					"type": "string",
					"minLength": 1
				},
				"fromName": {
					"type": "string",
					"minLength": 1
				}
			}
		}
	}
}
