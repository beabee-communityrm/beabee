.page-header
	h3 &nbsp;
ul.nav.nav-pills.nav-stacked
	li( class=( page == 'overview' ? 'active' : '' ) ): a( href='/members/' + member.id ) Overview
	li( class=( page == 'profile' ? 'active' : '' ) ): a( href='/members/' + member.id + '/profile' ) Profile
	li( class=( page == 'contribution' ? 'active' : '' ) ): a( href='/members/' + member.id + '/contribution' ) Contribution
	if ( member.otp.activated )
		li( class=( page == '2fa' ? 'active' : '' ) ): a( href='/members/' + member.id + '/2fa' ) 2FA
	li( class=( page == 'permissions' ? 'active' : '' ) ): a( href='/members/' + member.id + '/permissions' ) Roles
